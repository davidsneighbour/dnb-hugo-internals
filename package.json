{
  "name": "@dnb-org-gohugo-components/internals",
  "version": "0.0.2",
  "license": "MIT",
  "description": "Better internal templates for GoHugo",
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com>",
  "homepage": "https://github.com/dnb-org/components/tree/master/internals",
  "bugs": "https://github.com/dnb-org/components/issues",
  "devDependencies": {
    "@dnb-org/standard-version-config": "3.5.9"
  },
  "scripts": {
    "release": "npm run release:patch",
    "release:major": "standard-version --release-as major -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" && bin/hooks/postrelease",
    "release:minor": "standard-version --release-as minor -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" && bin/hooks/postrelease",
    "release:patch": "standard-version --release-as patch -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" && bin/hooks/postrelease"
  },
  "private": true
}
